{
  "name": "babel-plugin-react-data-test-camelcase",
  "description": "This plugin transforms a 'dataTest' attribute to a 'data-test' attribute in React components.",
  "version": "0.1",
  "main": "lib/index.js",
  "author": "elParking <antonio.juan@elparking.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "check": "yarn run lint && yarn run test",
    "build": "babel src --out-dir lib",
    "prepublish": "in-publish && yarn run build || :"
  },
  "files": [
    "README.md",
    "index.js",
    "lib"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:shopify/node",
      "plugin:shopify/esnext"
    ],
    "rules": {
      "prefer-destructuring": "off"
    }
  },
  "babel": {
    "presets": [
      [
        "shopify/node",
        {
          "version": "6.0.0"
        }
      ]
    ]
  },
  "jest": {
    "testRegex": "[\\w+]\\.test\\.js$",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-preset-shopify": "^16.0.2",
    "eslint": "^3.19.0",
    "eslint-plugin-shopify": "^16.0.1",
    "in-publish": "^2.0.0",
    "jest": "^20.0.4",
    "prettier": "^1.17.1"
  },
  "dependencies": {}
}
