{
  "name": "babel-plugin-react-data-test-camelcase",
  "description": "This plugin transforms a 'dataTest' attribute to a 'data-test' attribute in React components.",
  "version": "0.0.1",
  "main": "lib/index.js",
  "author": "elParking <antonio.juan@elparking.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "check": "yarn run lint && yarn run test",
    "build": "babel src --out-dir lib",
    "prepublish": "in-publish && yarn run build || :"
  },
  "files": [
    "README.md",
    "index.js",
    "lib"
  ],
  "eslintConfig": {
    "rules": {
      "prefer-destructuring": "off"
    },
    "plugins": [
      "jest",
      "import"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "jest": {
    "cache": false,
    "testRegex": "[\\w+]\\.test\\.js$",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.13.2",
    "babel-core": "^6.13.2",
    "babel-jest": "^19.0.0",
    "eslint": "^6.1.0",
    "in-publish": "^2.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.0.0"
  }
}
